<h1>List of <%= @count %> Dojos</h1>
<table class = "container">
	<tr>
		<th>Coding Dojo Branch</th>
		<th>Street Address</th>
		<th>City</th>
		<th>State</th>
		<th>Actions</th>
	</tr>
	<% if @dojos %>
	<% @dojos.each do |dojo| %>
	<tr>
		<td><%= dojo.branch %></td>
		<td><%= dojo.street %></td>
		<td><%= dojo.city %></td>
		<td><%= dojo.state %></td>
		<td>
			<a href="/dojos/<%= dojo.id %>">Show</a>
			<a href="/dojos/<%= dojo.id %>/edit">Edit</a>
			<a href="/dojos/<%= dojo.id %>" data-method="delete">Destroy</a>
		</td>
	</tr>
	<% end %>
	<% end %>
</table>

<a href="dojos/new" id = "link">New Dojo</a>